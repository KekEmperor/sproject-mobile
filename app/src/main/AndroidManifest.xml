<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sproject_mobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sprojectmobile"
        android:usesCleartextTraffic="true">
        <activity android:name=".MessagesActivity"></activity>
        <activity
            android:name=".LevelsActivity"
            android:label="@string/levels_activity_title"
            android:parentActivityName=".MenuActivity" />
        <activity
            android:name=".MenuActivity"
            android:label="@string/menu_activity_title" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/login_activity_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>